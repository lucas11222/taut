{
  "name": "taut-installer",
  "version": "1.0.0",
  "description": "Installer for Taut",
  "type": "module",
  "bin": {
    "taut-installer": "./cli.js",
    "taut": "./cli.js"
  },
  "scripts": {
    "build": "bun build ./deps.js --target node --format cjs --keep-names --outfile ./deps/deps.bundle.js && cp ./node_modules/esbuild-wasm/esbuild.wasm ./deps/esbuild.wasm"
  },
  "files": [
    "cli.js",
    "loader.js"
  ],
  "keywords": [
    "taut",
    "slack",
    "mod",
    "patcher",
    "installer"
  ],
  "license": "MIT",
  "dependencies": {
    "@electron/asar": "^4.0.1",
    "@electron/fuses": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/browser": "^4.53.3",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.10.1",
    "bun": "^1.3.3",
    "electron": "^39.2.4",
    "esbuild": "^0.27.0",
    "esbuild-wasm": "^0.27.0",
    "typescript": "^5.9.3"
  }
}
